{
  "name": "jucifi",
  "version": "0.0.1",
  "description": "Lending broker",
  "main": "index.js",
  "repository": "git@github.com:MikaelLazarev/jucicifi.git",
  "author": "Mikhail Lazarev <mikael.lazarev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "gen": "yarn postinstall",
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json' && typechain --target=web3-v1 'build/contracts/*.json'",
    "postinstall": "truffle compile && yarn generate-types",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.6",
    "@typechain/truffle-v5": "^2.0.2",
    "@typechain/web3-v1": "^1.0.0",
    "@types/bignumber.js": "^5.0.0",
    "@types/chai": "^4.2.11",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^7.0.2",
    "bignumber.js": "^9.0.1",
    "dotenv": "^8.2.0",
    "openzeppelin-solidity": "^3.2.0",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "prompt-sync": "^4.2.0",
    "solidity-coverage": "^0.7.11",
    "truffle": "^5.1.47",
    "truffle-hdwallet-provider-privkey": "^0.3.0",
    "truffle-typings": "^1.0.7",
    "ts-generator": "^0.0.8",
    "ts-node": "^9.0.0",
    "typechain": "^2.0.1",
    "typescript": "^4.0.3",
    "web3-core": "^1.3.0",
    "web3-eth-contract": "^1.3.0"
  }
}
