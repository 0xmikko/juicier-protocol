    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typechain/truffle-v5": "^2.0.2",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "openzeppelin-solidity": "^3.2.0",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "truffle": "^5.1.47",
    "truffle-typings": "^1.0.7",
    "ts-generator": "^0.0.8",
    "ts-node": "^9.0.0",
    "typechain": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
